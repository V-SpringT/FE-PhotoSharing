{
    "version": 2,
    "builds": [
      {
        "src": "./src/index.jsx",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "handle": "filesystem" },
      {
        "src": "routes/(.*)",
        "dest": "routes/$1"
      },
      {
        "src": "/(.*)",
        "dest": "./src/index.jsx"
      }
    ]
  }